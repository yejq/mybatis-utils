<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
	      PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
	      "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- 运行命令：mvn org.mybatis.generator:mybatis-generator-maven-plugin:1.3.1:generate -->
<generatorConfiguration>
	<!-- 配置mysql驱动的完整路径 -->
	<classPathEntry
		location="D:\J2EE\maven\repository\mysql\mysql-connector-java\5.1.30\mysql-connector-java-5.1.30.jar" />

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" /> <!-- 是否禁止注释 -->
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/gmsales?generateSimpleParameterMetadata=true"
			userId="username" password="password">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 路径的包必须事先创建 -->
		<!-- generate Model -->
		<javaModelGenerator targetPackage="com.yejq.generator.pojo"
			targetProject="E:\360\Learn\MyBatis\workspace\My\mybatis-utils\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- generate xml -->
		<sqlMapGenerator targetPackage="mapper"
			targetProject="E:\360\Learn\MyBatis\workspace\My\mybatis-utils\src\main\resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- generate Mapper -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.yejq.generator.mapper"
			targetProject="E:\360\Learn\MyBatis\workspace\My\mybatis-utils\src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="audit" domainObjectName="Audit"></table>
		<table schema="" enableDeleteByPrimaryKey="false"
			enableDeleteByExample="false" enableCountByExample="false"
			enableSelectByExample="false" enableUpdateByExample="false"
			tableName="channel" domainObjectName="Channel"></table>
	</context>
</generatorConfiguration>
